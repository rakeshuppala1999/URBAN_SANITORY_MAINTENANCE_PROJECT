<script lang="javascript">
function update()
{
   
   var a=prompt("Please Enter Your Complaint ID ",205);

	if(a!=null)
	{
	document.getElementbyId("b1").style.display="inline";
	return a=0;
	}
	else
	{
	document.getElementbyId("b1").style.display="none";
	return a=1;
	}
}

function del()
{
   
